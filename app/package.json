    "cytoscape-tidytree": "^0.2.0",
    "file-saver": "^2.0.5",
    "htl": "^0.3.1",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^19.0.0",
    "react-calendar": "^5.1.0",
    "react-date-picker": "^11.0.0",
    "react-dom": "^19.0.0",
    "react-hotkeys-hook": "^4.6.1",
    "react-resizable-panels": "^2.1.7",
    "tailwind-merge": "^2.6.0",
    "use-file-picker": "^2.1.2",
    "zod": "^3.24.1",
