{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "storybook:doctor": "storybook doctor",
    "storybook:build": "storybook build",
    "publish:all": "pnpm publish:app && pnpm publish:storybook",
    "publish:app": "pnpm build && tsx scripts/modify-build-urls && rm -rf ../assets && cp -r dist/* ../",
    "publish:storybook": "pnpm storybook:build && rm -rf ../storybook && mkdir ../storybook && cp -r storybook-static/* ../storybook"
  },
  "dependencies": {
    "@fire/forecast-engine": "workspace:^",
    "@floating-ui/react": "^0.27.0",
    "cytoscape": "^3.30.4",
    "cytoscape-all-paths": "^0.1.0",
    "cytoscape-tidytree": "^0.2.0",
    "htl": "^0.3.1",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-resizable-panels": "^2.1.7",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@eslint/js": "^9.17.0",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-themes": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/manager-api": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@storybook/theming": "^8.4.7",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "@types/cytoscape": "^3.21.8",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.4",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-storybook": "^0.11.2",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "storybook": "^8.4.7",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.19.0",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
