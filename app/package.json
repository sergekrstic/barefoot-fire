{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "storybook:doctor": "storybook doctor",
    "storybook:build": "storybook build",
    "publish:all": "pnpm publish:app && pnpm publish:storybook",
    "publish:app": "pnpm build && tsx scripts/modify-build-urls && rm -rf ../assets && cp -r dist/* ../",
    "publish:storybook": "pnpm storybook:build && rm -rf ../storybook && mkdir ../storybook && cp -r storybook-static/* ../storybook"
  },
  "dependencies": {
    "@fire/forecast-engine": "workspace:^",
